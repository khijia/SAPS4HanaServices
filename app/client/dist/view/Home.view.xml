<mvc:View
controllerName="tc.com.controller.Home"
	displayBlock="true"
	height="100%"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
   <Page title="{i18n>homePageTitle}" class="sapUiResponsiveContentPadding">
      <content>
         <Button text="{i18n>ItemsList}" class="sapUiTinyMarginEnd" press=".onNavToItemsList"/>
         <Button text="{i18n>SalesOrderList}" class="sapUiTinyMarginEnd" press=".onNavToSalesOrderList"/>
      </content>
   </Page>
</mvc:View>